    color:#E65100;
}

#timeAxis #clearBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 0;
    border-radius:6px;
    border:none;
    background-color:#FF6B6B;
    color:#FFFFFF;
    width:80px;
}

#timeAxis #clearBtn:hover{
    background-color:#FF5252;
}

#timeAxis #clearBtn:pressed{
    background-color:#FF3B3B;
}

#timeAxis #cancelBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 0;
    border-radius:6px;
    border:none;
    background-color:#C0C4CC;
    color:#FFFFFF;
    width:80px;
}

#timeAxis #cancelBtn:hover{
    background-color:#909399;
}

#timeAxis #cancelBtn:pressed{
    background-color:#606266;
}

#timeAxis #timeLabel{
    font-size:12px;
    font-weight:bold;
    color:#2D8CF0;
    text-align:center;
}

#timeAxis #timeAxisBtnDialog{
    background-color:#F5F7FA;
    border-radius:10px;
    border:none;
}

#timeAxis #timeAxisBtnDialog QLabel{
    font-size:13px;
    font-weight:bold;
    color:#2D8CF0;
    padding:6px 0;
    text-align:center;
}

#monthView #monthTitleLabel{
    font-size:15px;
    font-weight:bold;
    color:#2D8CF0;
    padding:0 10px;
}

#monthView #monthBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 10px;
    border-radius:6px;
    border:none;
    background-color:#FFFFFF;
    color:#333333;
}

#monthView #monthBtn:hover{
    background-color:#F0F0F0;
}

#monthView #monthBtn:pressed{
    background-color:#E0E0E0;
}

#monthView #currentMonthBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 10px;
    border-radius:6px;
    border:none;
    background-color:#2D8CF0;
    color:#FFFFFF;
}

#monthView #currentMonthBtn:hover{
    background-color:#1D7AD9;
}

#monthView #currentMonthBtn:pressed{
    background-color:#1D7AD9;
}

#monthView #statisticsBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 10px;
    border-radius:6px;
    border:none;
    background-color:#52C41A;
    color:#FFFFFF;
}

#monthView #statisticsBtn:hover{
    background-color:#4CAF50;
}

#monthView #statisticsBtn:pressed{
    background-color:#45a049;
}

#statsDlg{
    background-color:#FFFFFF;
    border-radius:10px;
    border:1px solid #EEEEEE;
}

#statsDlg QLabel{
    font-size:12px;
    color:#333333;
}

#statsDlg QGroupBox{
    font-size:14px;
    font-weight:bold;
    color:#333333;
    border:1px solid #DDDDDD;
    border-radius:6px;
    margin-top:10px;
    padding-top:15px;
}

#statsDlg QGroupBox::title{
    subcontrol-origin:margin;
    left:10px;
    padding:0 5px 0 5px;
}

#statsDlg QScrollArea{
    border:none;
}

#statsDlg QScrollArea QScrollBar:vertical{
    width:8px;
    background-color:#F5F7FA;
    border-radius:4px;
}

#statsDlg QScrollArea QScrollBar::handle:vertical{
    background-color:#C0C4CC;
    border-radius:4px;
}

#statsDlg QScrollArea QScrollBar::handle:vertical:hover{
    background-color:#909399;
}

#statsDlg #closeBtn{
    background-color:#2D8CF0;
    color:#FFFFFF;
    font-size:12px;
    padding:4px 20px;
    border-radius:4px;
    border:none;
}
