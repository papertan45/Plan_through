QMainWindow{
    background-color: #FFFFFF;
    border: none;
}

*{
    color:#333333;
}
QLabel{
    color:#333333;
}
QGroupBox{
    color:#333333;
    font-weight:bold;
}

DayView QProgressBar{
    border:none;
    border-radius:6px;
    height:22px;
    background-color:#F0F0F0;
    font-size:12px;
    font-weight:bold;
    color:#333333;
}
DayView QProgressBar::chunk{
    background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0 #2D8CF0,stop:1 #1D7CE0);
    border-radius:6px;
}

DayView>#targetHourShowLabel{
    font-size:12px;
    font-weight:bold;
    color:#27AE60;
    padding:0 6px;
}

DayView>#clearBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 12px;
    border-radius:6px;
    border:none;
    background-color:#FF6B6B;
    color:#FFFFFF;
}

DayView>#clearBtn:hover{
    background-color:#FF5252;
}

DayView>#clearBtn:pressed{
    background-color:#FF3B3B;
}

DayView #setTargetBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 12px;
    border-radius:6px;
    border:none;
    background-color:#27AE60;
    color:#FFFFFF;
}

DayView #setTargetBtn:hover{
    background-color:#219653;
}

DayView #setTargetBtn:pressed{
    background-color:#1E8845;
}

DayView>#progressGroup{
    font-size:13px;
    font-weight:bold;
    color:#2D8CF0;
    border:2px solid #ECF5FF;
    border-radius:8px;
    padding:8px;
    margin:0;
}

DayView #progressTitle{
    font-size:13px;
    font-weight:bold;
    color:#2D8CF0;
}

DayView #selectedDateLabel{
    font-size:12px;
    font-weight:bold;
    color:#2D8CF0;
    padding:0 3px;
}

DayView #todayStudyHourLabel{
    font-size:12px;
    font-weight:bold;
    color:#333333;
    padding:4px 0;
}

DayView #statsGroup{
    font-size:13px;
    font-weight:bold;
    color:#2D8CF0;
    border:2px solid #ECF5FF;
    border-radius:8px;
    padding:8px;
}

DayView #continuousDaysLabel,
DayView #maxContinuousDaysLabel,
DayView #completedProjectsLabel,
DayView #studyCheckLabel{
    font-size:12px;
    font-weight:bold;
    color:#555555;
    padding:2px;
}

DayView #timeAxisScroll{
    border:none;
    background-color:transparent;
}

DayView #timeAxisScroll QScrollBar:vertical{
    width:6px;
    background-color:#F5F7FA;
    border-radius:3px;
}

DayView #timeAxisScroll QScrollBar::handle:vertical{
    background-color:#C0C4CC;
    border-radius:3px;
}

DayView #timeAxisScroll QScrollBar::handle:vertical:hover{
    background-color:#909399;
}

#dateSelectDialog{
    background-color:#F5F7FA;
    border-radius:10px;
    border:none;
}

#dateSelectDialog QCalendarWidget{
    background-color:#FFFFFF;
    border-radius:6px;
    border:1px solid #ECF5FF;
}

#dateSelectDialog QPushButton{
    font-size:12px;
    font-weight:bold;
    padding:5px 15px;
    border-radius:6px;
    border:none;
    background-color:#2D8CF0;
    color:#FFFFFF;
    margin-top:8px;
}

#dateSelectDialog QPushButton:hover{
    background-color:#1D7AD9;
}

#setTargetDialog{
    background-color:#F5F7FA;
    border-radius:10px;
    border:none;
}

#setTargetDialog QLabel{
    font-size:13px;
    font-weight:bold;
    color:#27AE60;
    padding:6px 0;
    text-align:center;
}

#setTargetDialog #hourBtn{
    font-size:12px;
    padding:6px 0;
    border-radius:6px;
    border:none;
    background-color:#FFFFFF;
    color:#333333;
}

#setTargetDialog #hourBtn:hover{
    background-color:#F0F9F0;
    color:#27AE60;
}

#setTargetDialog #hourBtn:pressed{
    background-color:#27AE60;
    color:#FFFFFF;
    font-weight:bold;
}

#setTargetDialog cancelBtn{
    font-size:12px;
    font-weight:bold;
    padding:5px 0;
    border-radius:6px;
    border:none;
    background-color:#C0C4CC;
    color:#FFFFFF;
    width:80px;
    margin-top:3px;
}

#setTargetDialog cancelBtn:hover{
    background-color:#909399;
}

#setTargetDialog cancelBtn:pressed{
    background-color:#606266;
}

